    <script>
        var win = navigator.platform.indexOf('Win') > -1;
        if (win && document.querySelector('#sidenav-scrollbar')) {
            var options = {
                damping: '0.5'
            }
            Scrollbar.init(document.querySelector('#sidenav-scrollbar'), options);
        }

        function markAsRead(notificationId) {
            $.ajax({
                url: '@Url.Action("MarkNotificationAsRead", "Notification")',
                type: 'POST',
                data: { notificationId: notificationId },
                success: function (response) {
                    if (response.success) {
                        // Si la notification est marquée comme lue, on peut retirer l'élément ou changer son apparence
                        $('li[data-notification-id="' + notificationId + '"]').fadeOut();
                        // Recharger la page après la mise à jour
                        location.reload();
                    } else {
                        alert("Erreur lors de la mise à jour de la notification.");
                    }
                },
                error: function () {
                    alert("Erreur de communication avec le serveur.");
                }
            });
        }

        $(document).ready(function () {
            var notifications = $(".notification-item").get();
            notifications.sort(function (a, b) {
                var dateA = new Date($(a).data('createdat'));
                var dateB = new Date($(b).data('createdat'));
                return dateB - dateA;  // Tri décroissant : le plus récent en haut
            });
            $(".scrollable-notifications").html(notifications);  // Réorganiser les éléments
        });
        

    </script>

